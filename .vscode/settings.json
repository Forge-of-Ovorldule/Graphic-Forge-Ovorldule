{
  "makefile.configureOnOpen": true,
  "editor.stickyScroll.enabled": true,
  "breadcrumbs.enabled": true,
  "C_Cpp.autocomplete": "default",
  "C_Cpp.intelliSenseEngine": "default",
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": true
  },
  "cmake.configureOnOpen": true,
  "cmake.buildDirectory": "${workspaceFolder}/build",
  "cmake.generator": "Ninja",
  "debug.console.fontSize": 13,
  "debug.internalConsoleOptions": "openOnSessionStart",
  "C_Cpp.enhancedColorization": "enabled",
  "editor.semanticHighlighting.enabled": true,
  "keyboard.dispatch": "keyCode",
  "vim.useSystemClipboard": true,
  "vim.handleKeys": {
    "<C-c>": false,
    "<C-v>": false
  },
  "debug.allowBreakpointsEverywhere": true,
  "debug.showBreakpointsInOverviewRuler": true,
  "editor.folding": true,
  "editor.definitionLinkOpensInPeek": false,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "boundary",
  "editor.renderLineHighlight": "all",
  "vscode_custom_css.imports": [
    "file:///home/stepan/.vscodestyle.css"
  ],
  "vscode_custom_css.policy": true,
  "workbench.colorTheme": "Dracula Theme",
  "editor.fontFamily": "'JetBrains Mono', 'Fira Code', monospace",
  "editor.fontSize": 14,
  "editor.fontLigatures": true,
  "editor.smoothScrolling": false,
  "workbench.list.smoothScrolling": true,
  "window.titleBarStyle": "custom",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorBlinking": "smooth",
  "workbench.editor.enablePreview": false,
  "explorer.decorations.badges": true,
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.folders.theme": "specific",
  "material-icon-theme.folders.color": "#c678dd",
  "workbench.startupEditor": "none",
  "workbench.productIconTheme": "Default",
  "workbench.editor.revealIfOpen": true,
  "window.zoomLevel": 0,
  "editor.cursorSurroundingLines": 5,
  "editor.guides.bracketPairs": true,
  "workbench.tree.renderIndentGuides": "always",
  "C_Cpp.errorSquiggles": "enabled",
  "C_Cpp.inlayHints.autoDeclarationTypes.enabled": true,
  "C_Cpp.inlayHints.parameterNames.enabled": true,
  "C_Cpp.inlayHints.referenceOperator.enabled": true,
  "C_Cpp.inlayHints.includeDependency.enabled": true,
  "editor.inlayHints.enabled": "onUnlessPressed",
  "copilot.inlayHints.enabled": true,
  "copilot.inlayHints.parameterHints.enabled": true,
  "copilot.inlayHints.parameterHints.prefix": " âž¤ ",
  "copilot.inlayHints.typeHints.enabled": true,
  "editor.inlayHints.fontFamily": "'JetBrains Mono', 'Fira Code', monospace",
  "editor.inlayHints.fontSize": 13,
  "editor.inlayHints.padding": true,
  "editor.parameterHints.cycle": true,
  "editor.parameterHints.enabled": true,
  "C_Cpp_Runner.msvcBatchPath": ""
}